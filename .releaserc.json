{
  "branches": ["main"],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/changelog",
    [
      "@semantic-release/github",
      {
        "assets": [
          "release-artifacts/ocsight-darwin-x64.zip",
          "release-artifacts/ocsight-darwin-arm64.zip",
          "release-artifacts/ocsight-linux-x64.zip",
          "release-artifacts/ocsight-linux-arm64.zip"
        ]
      }
    ],
    "@semantic-release/npm",
    "@semantic-release/git"
  ]
}
