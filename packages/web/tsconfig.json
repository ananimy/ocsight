{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "strict": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "skipLibCheck": true,
    "baseUrl": ".",
    "moduleResolution": "bundler",
    "paths": {
      "@/*": ["src/*"],
      "@/components/*": ["src/components/*"],
      "@/lib/*": ["src/lib/*"],
      "@/pages/*": ["src/pages/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
